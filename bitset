#include <stdio.h>
#include <iostream>
#include <bitset>
using namespace std;

int main(int argc, char** argv) {
	int a,b;
	
	cout<<"Podaj A: \n";
	cin>>a;
	cout<<"Podaj B: \n"<<"\n";
	cin>>b;
	
	cout<<"A: "<<a<<"\n";
	cout<<"B: "<<b<<"\n"<<"\n";

	cout<<"A^B(2)= "<<bitset<8>(a^b)<<"\n";
	
    cout<<"A|B(2)= "<<bitset<8>(a|b)<<"\n";

    cout<<"A&B(2)= "<<bitset<8>(a&b)<<"\n";

    cout<<"~A(2)= "<<bitset<8>(~a)<<"\n";

    cout<<"~B(2)= "<<bitset<8>(~b)<<"\n";

    cout<<"(2)A<<=1 "<<bitset<8>(a<<=1)<<"\n";

    cout<<"(2)B>>=1 "<<bitset<8>(b>>=1)<<"\n"<<"\n"<<"\n";
    
    
    
    cout<<"A^B(10)= "<<bitset<10>(a^b)<<"\n";
	
    cout<<"A|B(10)= "<<bitset<10>(a|b)<<"\n";

    cout<<"A&B(10)= "<<bitset<10>(a&b)<<"\n";

    cout<<"~A(10)= "<<bitset<10>(~a)<<"\n";

    cout<<"~B(10)= "<<bitset<10>(~b)<<"\n";

    cout<<"(10)A<<=1 "<<bitset<10>(a<<=1)<<"\n";

    cout<<"(10)B>>=1 "<<bitset<10>(b>>=1)<<"\n";
	
	return 0;
}
